1. create expense_tbl:
CREATE TABLE IF NOT EXISTS `expense_tbl`(`SEQ` INT(10) NOT NULL,   `DATE` DATE NOT NULL,   `SUBCAT_ID` INT(3) NOT NULL,   `AMT` FLOAT NOT NULL,   `CURRENCY_ID` INT(1) NOT NULL,   PRIMARY KEY ( `SEQ` ))ENGINE=InnoDB DEFAULT CHARSET=utf8;

2. create income_tbl:
CREATE TABLE IF NOT EXISTS `income_tbl`(`SEQ` INT(10) NOT NULL,   `DATE` DATE NOT NULL,   `SUBCAT_ID` INT(3) NOT NULL,   `AMT` FLOAT NOT NULL,   `CURRENCY_ID` INT(1) NOT NULL,   PRIMARY KEY ( `SEQ` ))ENGINE=InnoDB DEFAULT CHARSET=utf8;
