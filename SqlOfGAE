1. create expense_tbl:
CREATE TABLE IF NOT EXISTS `expense_tbl`(`SEQ` INT(10) NOT NULL,   `DATE` DATE NOT NULL,   `SUBCAT_ID` INT(3) NOT NULL,   `AMT` FLOAT NOT NULL,   `CURRENCY_ID` INT(1) NOT NULL,   PRIMARY KEY ( `SEQ` ))ENGINE=InnoDB DEFAULT CHARSET=utf8;

2. create income_tbl:
CREATE TABLE IF NOT EXISTS `income_tbl`(`SEQ` INT(10) NOT NULL,   `DATE` DATE NOT NULL,   `SUBCAT_ID` INT(3) NOT NULL,   `AMT` FLOAT NOT NULL,   `CURRENCY_ID` INT(1) NOT NULL,   PRIMARY KEY ( `SEQ` ))ENGINE=InnoDB DEFAULT CHARSET=utf8;

3. create subcat_tbl:
CREATE TABLE IF NOT EXISTS `subcat_tbl`(   `SUBCAT_ID` INT(3) NOT NULL,   `SUBCAT_EN` VARCHAR(10) NOT NULL,   `SUBCAT_CN` VARCHAR(20) NOT NULL,   `TYPE_ID` INT(1) NOT NULL,   PRIMARY KEY ( `SUBCAT_ID` ))ENGINE=InnoDB DEFAULT CHARSET=utf8;

4. create currency_tbl:
CREATE TABLE IF NOT EXISTS `currency_tbl`(   `CURRENCY_ID` INT(3) NOT NULL,   `CURRENCY_EN` VARCHAR(3) NOT NULL,   `CURRENCY_CN` VARCHAR(10) NOT NULL,   PRIMARY KEY ( `CURRENCY_ID` ))ENGINE=InnoDB DEFAULT CHARSET=utf8;

